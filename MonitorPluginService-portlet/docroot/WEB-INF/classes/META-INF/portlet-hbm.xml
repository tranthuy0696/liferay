<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.service.serviceBuilder.model.Cpu" />
	<import class="com.service.serviceBuilder.model.Ram" />
	<class name="com.service.serviceBuilder.model.impl.CpuImpl" table="Cpu">
		<id name="cpuId" type="long">
			<generator class="increment" />
		</id>
		<property name="time" type="org.hibernate.type.TimestampType" column="time_" />
		<property name="min" type="com.liferay.portal.dao.orm.hibernate.DoubleType" />
		<property name="max" type="com.liferay.portal.dao.orm.hibernate.DoubleType" />
		<property name="aver" type="com.liferay.portal.dao.orm.hibernate.DoubleType" />
	</class>
	<class name="com.service.serviceBuilder.model.impl.RamImpl" table="Ram">
		<id name="ramId" type="long">
			<generator class="increment" />
		</id>
		<property name="time" type="org.hibernate.type.TimestampType" column="time_" />
		<property name="min" type="com.liferay.portal.dao.orm.hibernate.DoubleType" />
		<property name="max" type="com.liferay.portal.dao.orm.hibernate.DoubleType" />
		<property name="aver" type="com.liferay.portal.dao.orm.hibernate.DoubleType" />
	</class>
</hibernate-mapping>